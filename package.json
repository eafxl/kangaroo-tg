{
  "name": "kangaroo-tg",
  "version": "1.0.0",
  "description": "kangaroo notification utils",
  "author": "eafx",
  "private": true,
  "license": "MIT License",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "pnpm test --passWithNoTests",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "eslint": "eslint './src/**/*.+(js|ts|tsx)' --quiet",
    "eslintall": "eslint './src/**/*.+(js|ts|tsx)'",
    "eslintfix": "eslint './src/**/*.+(js|ts|tsx)' --fix",
    "build": "tsc -p tsconfig.build.json",
    "rebuild": "rimraf dist && tsc -p tsconfig.build.json",
    "test": "jest --config ./jest.config.js --logHeapUsage --runInBand",
    "clear_jest": "jest --clearCache",
    "test-e2e": "jest --config ./jest-e2e.config.js --runInBand",
    "debug-jest": "node --nolazy --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --testTimeout=1800000",
    "debug-test": "pnpm debug-jest --config ./jest.config.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "postinstall": "npx playwright install --with-deps"
  },
  "dependencies": {
    "@nestjs/common": "10.2.7",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.7",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.7",
    "@nestjs/swagger": "7.1.13",
    "@nestjs/typeorm": "10.0.0",
    "@types/jsdom": "^21.1.5",
    "@types/lodash": "^4.14.202",
    "@types/node-telegram-bot-api": "^0.61.10",
    "axios": "^1.6.2",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "handlebars": "^4.7.8",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "multer": "1.4.4",
    "node-telegram-bot-api": "^0.63.0",
    "nodemailer": "6.9.6",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "playwright": "^1.40.1",
    "playwright-chromium": "^1.40.1",
    "puppeteer": "^21.5.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.7",
    "@types/bcryptjs": "2.4.4",
    "@types/express": "4.17.18",
    "@types/jest": "29.5.5",
    "@types/ms": "0.7.32",
    "@types/multer": "1.4.8",
    "@types/node": "18.18.1",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.10",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "2.0.13",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "env-cmd": "10.1.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.1",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-circus": "^29.4.x",
    "jest-config": "^29.4.x",
    "jest-date-mock": "^1.0.8",
    "jest-html-reporters": "^3.1.4",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "jest-environment-node": "29.x"
  }
}
